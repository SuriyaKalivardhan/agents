{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "AIUserFunctionHost",
      "storageProvider": {
        "maxQueuePollingInterval": "00:00:05",
        "workItemQueueVisibilityTimeout": "00:00:05",
        "controlQueueVisibilityTimeout": "00:01:00"
      },
      "maxEntityOperationBatchSize": 1,
      "useAppLease": false,
      "useTablePartitionManagement": true,
      "extendedSessionsEnabled": true,
      "extendedSessionIdleTimeoutInSeconds": 30      
    },
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        },
        "enableLiveMetricsFilters": true
      }
    }
  }
}